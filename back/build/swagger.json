				"operationId": "DeleteHistory",
				"responses": {
					"204": {
						"description": "Supprimé"
					},
					"404": {
						"description": "Non trouvé"
					},
					"500": {
						"description": "Erreur serveur"
					}
				},
				"summary": "Supprimer un historique",
				"tags": [
					"Histories"
				],
				"security": [],
				"parameters": [
					{
						"in": "path",
						"name": "id",
						"required": true,
						"schema": {
							"format": "double",
							"type": "number"
						}
					}
				]
