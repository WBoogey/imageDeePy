{
  "name": "image-dee-py-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "dev": "pnpm -r dev",   
    "build": "pnpm -r build",    
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "dev:front": "pnpm --filter front dev",
    "dev:back": "pnpm --filter back dev",
    "dev:all": "pnpm --parallel --filter \"front...\" \"back...\" dev"
  },
  "devDependencies": {
    "eslint": "catalog:",
    "prettier": "^3.5.3",
    "typescript": "catalog:",
    "@types/node": "^22"
  }
}
